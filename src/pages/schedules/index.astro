---
import Layout from "@/layouts/Layout.astro";
import Authorized from "@/layouts/Authorized.astro";

// components
import ScheduleList from "@/components/schedules/ScheduleList.vue";
import { getSchedules } from "../api/api";

const schedules = await getSchedules(Astro.request)
---
<Layout title="Schedules list">
  <Authorized>
    <ScheduleList 
      client:only="vue"
      schedules={schedules} 
    />
  </Authorized>
</Layout>